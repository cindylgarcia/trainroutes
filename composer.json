{
    "name": "drupal/recommended-project",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "daemth/easychart",
                "version": "v3.1.3",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "easychart"
                },
                "dist": {
                    "url": "https://github.com/daemth/easychart/archive/3.1.3.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "highcharts/highcharts",
                "version": "v10.2.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "highcharts"
                },
                "dist": {
                    "url": "https://code.highcharts.com/zips/Highcharts-10.2.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-youtube/ckeditor-youtube",
                "version": "v2.1.18",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor-youtube"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/youtube/releases/youtube_2.1.18.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "us-map/us-map",
                "version": "v1.0.1",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "usmap"
                },
                "dist": {
                    "url": "https://github.com/NewSignature/us-map/archive/refs/tags/v1.0.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "daemth/easychart": "^3.1",
        "highcharts/highcharts": "^10.2",
        "drupal/core-composer-scaffold": "9.4.7",
        "drupal/core-recommended": "9.4.7",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^11.2",
        "drupal/address": "^1.2",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_admin_toolbar": "^1.4.0",
        "drupal/adminimal_theme": "^1.3.0",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/better_formats": "1.x-dev",
        "drupal/cdn": "^3.1",
        "drupal/ckeditor_youtube": "^1.1.0",
        "drupal/components": "^2.4",
        "drupal/config_ignore": "^2.2",
        "drupal/config_pages": "^2.1.0",
        "drupal/config_split": "^1.7",
        "drupal/core-project-message": "9.4.7",
        "drupal/crop": "^2.0",
        "drupal/ctools": "^3.0.0",
        "drupal/default_content": "^1.0@alpha",
        "drupal/devel": "^4",
        "drupal/disqus": "^1.0",
        "drupal/easychart": "^4.0.2",
        "drupal/entity": "^1.4",
        "drupal/entity_browser": "^2.0",
        "drupal/entity_embed": "^1.0.0-beta2",
        "drupal/entity_reference_revisions": "^1.3.0",
        "drupal/entityqueue": "^1.0.0-alpha6",
        "drupal/facets": "^2.0",
        "drupal/field_group": "^3.3",
        "drupal/file_entity": "^2.0@beta",
        "drupal/focal_point": "^1.0",
        "drupal/google_analytics_counter": "^3.x-dev",
        "drupal/image_widget_crop": "^2.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/linkit": "^6.0@beta",
        "drupal/maxlength": "^1.0.0-beta1",
        "drupal/media_entity_generic": "^1.0",
        "drupal/memcache": "^2.5",
        "drupal/menu_link_config": "^1.0@alpha",
        "drupal/metatag": "^1.22",
        "drupal/metatag_routes": "^1.1",
        "drupal/migrate_file": "^2.0",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/migrate_tools": "^5.0",
        "drupal/name": "^1.0@RC",
        "drupal/oembed_lazyload": "^1.0@alpha",
        "drupal/paragraphs": "^1.15",
        "drupal/path_redirect_import": "^1.0@beta",
        "drupal/pathauto": "^1.0.0",
        "drupal/purge": "^3.4",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.8",
        "drupal/redis": "^1.0@RC",
        "drupal/search_api": "^1.4",
        "drupal/search_api_solr": "^4.2.1",
        "drupal/search_autocomplete": "^2.0",
        "drupal/simple_sitemap": "^3.1",
        "drupal/smart_trim": "^1.0.0",
        "drupal/stage_file_proxy": "^1.1",
        "drupal/title": "^2.x-dev",
        "drupal/token": "^1.0.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/views_infinite_scroll": "^1.3.0",
        "drupal/webform": "^6.0",
        "drupal/workbench_moderation": "^1.2.0",
        "solarium/solarium": "^6.1.5",
        "symfony/intl": "^4.0",
        "webflo/drupal-finder": "^1.0",
        "d8-contrib-modules/cloudflarephpsdk": "1.0.0-alpha5",
        "drupal/cloudflare": "^1.0@beta",
        "ckeditor-youtube/ckeditor-youtube": "^2.1",
        "us-map/us-map": "^1.0",
        "drupal/embed": "^1.6",
        "drupal/jquery_ui_touch_punch": "^1.1",
        "drupal/protected_pages": "^1.6",
        "drupal/entity_access_password": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^9.2",
        "symfony/css-selector": "~4.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.3",
        "mglaman/phpstan-drupal": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.1"
    },
    "conflict": {
            "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": false,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/embed": {
                "Allow site-builders to use a non-managed file as button icon.": "https://www.drupal.org/files/issues/2019-12-30/embed-allow-non-managed-button-icon-files--2824110-39_0.patch"
            },
            "drupal/core": {
                "Error when a process plugin gives a multiple value for a subproperty destination": "https://www.drupal.org/files/issues/2789125-subproperties-with-multiple-process-plugin-5.patch"
            },
            "drupal/views": {
                "Removes unwanted notices when using a date field as an exposed filter.": "https://www.drupal.org/files/issues/2811887-30-move-restore-overwritten-by-parent.patch"
            },
            "drupal/cloudflare": {
                "Purge URLS": "https://www.drupal.org/files/issues/2020-05-27/cloudflare_purging_by_url-2965749-5.patch"
            },
            "drupal/smart_trim": {
                "3013628 - Maximum function nesting level reached for elements with many children": "https://www.drupal.org/files/issues/2018-11-14/smart_trim-3013628_1.patch"
            },
          
            "drupal/title": {
                "Dependency on a non-existent service entity manager": "https://www.drupal.org/files/issues/2021-12-29/title-non_existent_service_dependency-3228651-3.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\Core\\Composer\\": "core/lib/Drupal/Core/Composer"
        }
    },
    "scripts": {
        "pre-update-cmd": [
            "DrupalComposerManaged\\ComposerScripts::preUpdate"
        ],
        "upstream-require": [
            "DrupalComposerManaged\\ComposerScripts::upstreamRequire"
        ]
    },
    
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true
        }
    }
}
